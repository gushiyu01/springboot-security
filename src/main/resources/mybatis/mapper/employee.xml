<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gsy.security.mapper.EmployeeMapper">
    <select id="findEmpById" resultType="com.gsy.security.entity.Employee">
      select * from employee where id = #{id}
    </select>
</mapper>